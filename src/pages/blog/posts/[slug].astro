export async function getStaticPaths() {
const res = await fetch("https://tcwk2rc4.directus.app/items/blog_posts/3?fields=content").then(r => r.json());
const post = await res.data.content;

	return {
		params: {
			slug: "b"
		},
		props: {
			post: post
		}
	}
}

const { Content } = Astro.props.post
