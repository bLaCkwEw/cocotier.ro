---
import { webVitals } from "@components/helpers/vitals"

const { analyticsId = import.meta.env.VERCEL_ANALYTICS_ID } = Astro.props;
---

<script define:vars={{analyticsId}}>
	if (analyticsId) {
		webVitals({
			path: Astro.url.pathname,
			params: Astro.url.searchParams.entries(),
			analyticsId
		});
	}
</script>